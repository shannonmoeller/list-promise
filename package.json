{
  "name": "list-promise",
  "version": "1.1.0",
  "description": "Maximally-concurrent Promise-based array manipulation.",
  "main": "lib/list-promise.js",
  "scripts": {
    "coveralls": "nyc report -r text-lcov | coveralls",
    "build": "rm -rf lib && babel src --out-dir lib",
    "prepublish": "npm test && npm run build",
    "pretest": "xo {src,test}/**/*.js",
    "test": "nyc ava -v",
    "posttest": "nyc report -r lcov",
    "watch": "chokidar '{src,test}/**' -c 'npm test'"
  },
  "repository": "shannonmoeller/list-promise",
  "keywords": [],
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shannonmoeller/list-promise/issues"
  },
  "homepage": "https://github.com/shannonmoeller/list-promise",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-plugin-async-to-promises": "^1.0.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.6",
    "nyc": "^6.0.0",
    "xo": "^0.13.0"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "async-to-promises"
    ]
  },
  "nyc": {
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "xo": {
    "rules": {
      "brace-style": [
        2,
        "stroustrup"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-eq-null": 0,
      "object-curly-spacing": [
        2,
        "always"
      ],
      "operator-linebreak": [
        2,
        "before"
      ],
      "babel/object-curly-spacing": [
        2,
        "always"
      ]
    }
  }
}
